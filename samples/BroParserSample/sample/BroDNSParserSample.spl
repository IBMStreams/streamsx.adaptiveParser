namespace sample ;

use com.ibm.streamsx.adaptiveparser.bro::* ;

composite BroDNSParserSample {
	graph
		stream<rstring data> File = FileSource(){
			param
				file : "dns.03_00_00-04_00_00.log" ;
				format : line ;
		}

		stream<BroDNS_t> BroRecord = BroDNSParser(File) {
			param
				dataAttr : data ;
		}

		()as DSink = FileSink(BroRecord) {
			param
				file : "/dev/stdout" ;
				flush : 1u ;
				format : txt ;
		}

}


